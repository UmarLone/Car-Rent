<script>


    function PostComments() {

        if ($('#EmailId').val() == '') {

            swal("Email empty","Please enter your email address.","error");
       

            return false;
        }

        else {

            var Criteria = JSON.stringify({ Email: $('#EmailId').val() });
            $.ajaxSetup({ cache: false });
            $.ajax({
                type: "POST",
                url: '@Url.Action("Reply","Home")',
                data: Criteria,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                error: function (error) {
               alert('Error:' + error.responseText);
                },
                success: function (data) {
                    if (parseInt(data) > 0) {
                        $("#EmailId").val('');
                       swal("Success","Thanks for subscribing.","success");
                    }
                    else if (data.result == false) {
                       swal("Sorry","Subscription failed.","error");
                    }
                }
            });
        }
    };

 
    $(window).load(function() {
		
		$(".se-pre-con").fadeOut("slow");;
	});






	</script>








	

    



