	    /*************************************************
        The below script to validate decimal number for percentage
        **************************************************/
	    function validateDecimalPercentOnly(value) {
	        var _txtVal = 0;
	        if (value == null || value == '') {
	            _txtVal = 0;
	        }
	        else {
	            _txtVal = value;
	        }

	        if (parseFloat(_txtVal) < 100) {
	            return true;
	        }
	        else {
	            alert('Percentage must be between 0 to 100');
	            return false;
	        }
	    }
