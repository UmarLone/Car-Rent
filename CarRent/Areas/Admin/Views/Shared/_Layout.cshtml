	    function validateAlphaNumericOnly(e) {

	        var unicode = e.charCode ? e.charCode : e.keyCode;
	        if ((unicode == 8) || (unicode == 9) || (unicode == 32) || (unicode == 127) || (unicode > 96 && unicode < 123) || (unicode > 64 && unicode < 91) || (unicode > 47 && unicode < 58) || unicode == 37 || unicode == 39) {
	            if (unicode == 13)
	                $(".button").click();
	            return true;
	        }
	        else {
	            return false;
	        }

	    }

