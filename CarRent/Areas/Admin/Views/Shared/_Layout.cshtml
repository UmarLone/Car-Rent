	    function validateAlphaNumericOnlyOnSearch(e, btnId) {


	        var unicode = e.charCode ? e.charCode : e.keyCode;
	        if ((unicode == 8) || (unicode == 13) || (unicode == 46) || (unicode == 64) || (unicode == 95) || (unicode == 45) || (unicode == 9) || (unicode == 32) || (unicode == 127) || (unicode > 96 && unicode < 123) || (unicode > 64 && unicode < 91) || (unicode > 47 && unicode < 58) || unicode == 37 || unicode == 39) {
